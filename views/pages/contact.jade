extends ../layouts/application
block title
  | Contact
block content
  form(action="/contact", method="post", class="form")
    fieldset
      legend Contact Us
      label Name
      input(type="text", name="name")
      label Email
      input(type="text", name="email")
      label Body
      textarea(name="body")
      .form-actions
        input(type="submit", class="btn btn-primary", value="Contact Us")
  p #{new Date()}